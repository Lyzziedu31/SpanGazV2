@model SpanGazV2.Models.tbl_607_order_details

@{
    ViewBag.Title = "Delete";
}

<h2 class="alert alert-danger">Are you sure you want to delete this?</h2>

<div>
    <dl class="dl-horizontal">
        <dt>
            <a style="color:#ffbe2e">Poste number</a>
        </dt>

        <dd class="form-control col-md-3">
            @Html.DisplayFor(model => model.poste_number)
        </dd>

        <dt>
            <a style="color:#ffbe2e">Contract number</a>
        </dt>

        <dd class="form-control col-md-3">
            @Html.DisplayFor(model => model.tbl_607_order.order_number)
        </dd>
    </dl>
    <div class="row">
        @using (Html.BeginForm())
        {
            @Html.AntiForgeryToken()

            <div class="form-actions no-color">
                <button type="submit" class="btn btn-danger" value="Delete">Delete</button> |
            </div>
        }
        <div>
            <button class="btn btn-success" style="margin-left:15px" onclick="GetPostes()">Back to list</button>
        </div>
    </div>
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
